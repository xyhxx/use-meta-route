{
  "name": "use-meta-route",
  "version": "0.1.0",
  "description": "rewrite useRoutes of react-router-dom to support route meta and route guard",
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:cover": "vitest --coverage"
  },
  "main": "umd/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "files": [
    "es/",
    "umd/",
    "types/"
  ],
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./umd/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xyhxx/use-meta-route.git"
  },
  "keywords": [
    "react",
    "react-router",
    "react-router-meta",
    "react-router-guard"
  ],
  "author": "xyhxx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xyhxx/use-meta-route/issues"
  },
  "homepage": "https://github.com/xyhxx/use-meta-route#readme",
  "peerDependencies": {
    "react": ">=16.8",
    "react-router-dom": ">=6"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.16.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-react": "^2.0.0",
    "c8": "^7.12.0",
    "editorconfig": "0.15.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-proste": "^2.12.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vite-plugin-dts": "^1.4.0",
    "vitest": "^0.19.1"
  }
}
